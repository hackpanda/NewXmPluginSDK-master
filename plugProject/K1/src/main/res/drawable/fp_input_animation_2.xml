    <item android:drawable="@drawable/fp_input_2_3" android:duration="10"></item>
    <item android:drawable="@drawable/fp_input_2_4" android:duration="10"></item>
    <item android:drawable="@drawable/fp_input_2_5" android:duration="10"></item>
    <item android:drawable="@drawable/fp_input_2_6" android:duration="10"></item>
    <item android:drawable="@drawable/fp_input_2_7" android:duration="10"></item>
    <item android:drawable="@drawable/fp_input_2_8" android:duration="10"></item>
    <item android:drawable="@drawable/fp_input_2_9" android:duration="10"></item>
    <item android:drawable="@drawable/fp_input_2_10" android:duration="10"></item>
    <item android:drawable="@drawable/fp_input_2_11" android:duration="10"></item>
    <item android:drawable="@drawable/fp_input_2_12" android:duration="10"></item>
    <item android:drawable="@drawable/fp_input_2_13" android:duration="10"></item>
    <item android:drawable="@drawable/fp_input_2_14" android:duration="10"></item>
