<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.xiaomi.zkplug">

    <application>

        <!-- 支持插件的最低版本，必须 -->
        <meta-data
            android:name="minPluginSdkApiVersion"
            android:value="2" />
        <!-- 设备model，必须 -->
        <meta-data
            android:name="model"
            android:value="shjszn.lock.c1" />
        <!-- 设备处理消息入口，必须 -->
        <meta-data
            android:name="message_handler"
            android:value="com.xiaomi.zkplug.MessageReceiver" />
        <!-- 开发者Id, 必须 -->
        <meta-data
            android:name="MiHomeDeveloperId"
            android:value="id_1139628820" />
        <!-- 支持的运行平台,必须 -->
        <meta-data
            android:name="MiHomePlatform"
            android:value="phone" />
        <!-- 是否支持widget显示,非必须 -->
        <meta-data
            android:name="MiHomeSupportWidget"
            android:value="false" />

        <activity android:name=".member.MemberManageActivity" />
        <activity android:name=".member.MemberDetailActivity" />
        <activity android:name=".pwdset.PwdSetActivity" />
        <activity android:name=".authorization.AuthSearchActivity"></activity>
        <activity android:name=".authorization.AuthRemoveActivity"></activity>
        <activity android:name=".authorization.AuthGaveActivity"></activity>
        <activity android:name=".fpinput.FpInputActivity"></activity>
        <activity android:name=".fpdel.FpDelActivity"></activity>
        <activity android:name=".gateway.GateWayActivity"></activity>
    </application>

</manifest>