<?xml version="1.0" encoding="UTF-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >

    <item android:id="@android:id/background">
        <shape>
            <corners android:radius="8.0dip" />

            <gradient
                android:angle="270.0"
                android:endColor="#ffdfdfdf"
                android:startColor="#ffdfdfdf" />
        </shape>
    </item>
    <item android:id="@android:id/secondaryProgress">
        <shape>
            <corners android:radius="8.0dip" />

            <gradient
                android:angle="270.0"
                android:endColor="#ffdfdfdf"
                android:startColor="#ffdfdfdf" />
        </shape>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <corners android:radius="8.0dip" />

                <gradient
                    android:angle="270.0"
                    android:centerColor="#ff01db91"
                    android:endColor="#ff01db91"
                    android:startColor="#ff01db91" />
            </shape>
        </clip>
    </item>

</layer-list>