<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#2889fd"
    android:orientation="vertical"
    tools:context=".main.MainActivity">
    <include layout="@layout/title_bar_transparent_white" />
    <LinearLayout
        android:background="#eeeeee"
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <ImageView
            android:layout_width="@dimen/x64"
            android:layout_gravity="center_horizontal"
            android:layout_marginTop="@dimen/y60"
            android:background="@drawable/icon_wo"
            android:layout_height="@dimen/x64" />
        <TextView
            android:id="@+id/memberName"
            android:gravity="center_horizontal"
            android:text="二月花"
            android:textColor="#000000"
            android:alpha="0.5"
            android:textSize="@dimen/x13"
            android:layout_marginTop="@dimen/y17"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" />
        <ImageView
            android:id="@+id/authRemoveBtn"
            android:layout_gravity="center_horizontal"
            android:background="@drawable/auth_remove_selector"
            android:layout_marginTop="@dimen/y227"
            android:layout_width="@dimen/x279"
            android:layout_height="@dimen/y25" />
    </LinearLayout>


</LinearLayout>