<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#2889fd"
    android:orientation="vertical"
    tools:context="com.xiaomi.zkplug.member.MemberManageActivity">
    <include layout="@layout/title_bar_transparent_white" />
    <LinearLayout
        android:background="#eeeeee"
        android:layout_width="match_parent"
        android:orientation="vertical"
        android:layout_centerHorizontal="true"
        android:layout_height="match_parent">
        <RelativeLayout
            android:id="@+id/ownerRel"
            android:background="@drawable/item_selector"
            android:layout_width="match_parent"
            android:layout_height="@dimen/y38">
            <TextView
                android:text="姓名"
                android:layout_centerVertical="true"
                android:layout_marginLeft="@dimen/x20"
                android:textSize="@dimen/x13"
                android:textColor="#000000"
                android:alpha="0.8"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />
            <ImageView
                android:id="@+id/entryImg"
                android:layout_centerVertical="true"
                android:layout_marginRight="@dimen/x21"
                android:layout_alignParentRight="true"
                android:layout_width="@dimen/x6"
                android:background="@drawable/icon_xiayiceng"
                android:layout_height="@dimen/y10" />
        </RelativeLayout>
        <ListView
            android:id="@+id/memListView"
            android:layout_marginTop="@dimen/y29"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

        </ListView>
    </LinearLayout>
</LinearLayout>
